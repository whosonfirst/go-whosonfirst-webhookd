{
    "daemon": "lambda://",
    "receivers": {
	"insecure": "insecure://",
	"github_index" : "github://?secret={SECRET}&ref=refs/heads/master"		
    },	
    "transformations": {
	"null": "null://",
	"chicken": "chicken://zxx",
	"clucking": "chicken://eng?clucking=true",
	"commits": "githubcommits://",
	"repo" : "githubrepo://"	
    },
    "dispatchers": {
	"null": "null://",
	"log": "log://",
	"blob": "file:///tmp/webhookd/?metadata=skip",
	"indexing" : "lambda://{FUNCTION}?dsn=credentials={CREDENTIALS}%20region={REGION}"	
    },
    "webhooks": [
	{
	    "endpoint": "/insecure-test",
	    "receiver": "insecure",
	    "transformations": [ "commits" ],
	    "dispatchers": [ "log", "blob" ]
	},
	{
	    "endpoint": "/indexing-test/{SECRET}",
	    "receiver": "github_index",
	    "transformations": [ "repo" ],
	    "dispatchers": [ "indexing" ]
	}		
    ]
}
